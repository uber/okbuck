@args (
String ruleType,
String mavenCoords,
String sha256,
String sourcesSha256,
boolean enableJetifier,
boolean firstLevel,
)
@(ruleType)(
    name = "@name",
@if (enableJetifier) {
    enable_jetifier = True,
}
@if (firstLevel) {
    first_level = True,
}
@if (valid(labels)) {
    labels = [
    @for (label : sorted(labels)) {
        "@label",
    }
    ],
}
    maven_coords = "@mavenCoords",
    sha256 = "@sha256",
@if (valid(sourcesSha256)) {
    sources_sha256 = "@sourcesSha256",
}
@if (valid(deps)) {
    deps = [
    @for (dep : sorted(deps)) {
        "@dep",
    }
    ],
}
)
